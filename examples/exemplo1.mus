print "\n..Going to beacon\n";

while groundType != bn do
    NUM linearPower = 80.0;
    NUM angularPower = 1 * beaconAngle bn;
    move linearPower - angularPower, linearPower + angularPower; # or MOVE AT ja faz o apply
