NUM position= 0;
ROBOT Grimmy = ("Grimmy", position);

print "Going to beacon 1\n";

NUM bn = 1;
print "..Rotating to beacon\n";

rotate 80 until -10 < beaconAngle bn < 10; #ja faz o apply
