#Exemplo - Pretende-se criar um desafio para o robo descobrir o mapa. Para tal, precisa-se definir um mapa. 
#	(NÃ£o precisa de definir pontos de chegada ou de passagem)

	LABIRINTO (NAME = "lab03" WIDTH = 28  HEIGHT = 14) { 
		/* Definir mapa*/
		ROW 12 : "           |              |              ";
		ROW 11 : "--+--+--+--+  +--+--+--+  +  +  +  +  +  ";
		ROW 10 : "              |        |  |              ";
		ROW 9  : "  +--+--+--+--+  +--+--+  +--+  +  +  +  ";
		ROW 8  : "     |           |     |     |           ";
		ROW 7  : "--+  +  +  +  +--+  +  +--+  +--+  +  +  ";
		ROW 6  : "  |  |        |     |  |  |     |        ";
		ROW 5  : "--+  +--+--+--+  +--+  +--+--+  +  +  +  ";
		ROW 4  : "     |           |  |  |        |        ";
		ROW 3  : "  +  +  +--+--+--+  +  +  +--+--+  +  +  ";
		ROW 2  : "  |  |  |           |     |              ";
		ROW 1  : "  +--+  +  +  +  +  +--+--+  +  +  +  +  ";
		ROW 0  : "        |                                ";
	};
        
